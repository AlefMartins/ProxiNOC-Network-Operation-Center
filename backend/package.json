{
    "name": "tacacs-ldap-manager-backend",
    "version": "1.0.0",
    "description": "Backend para gerenciamento de acessos via Tacacs+ e LDAP",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "db:migrate": "sequelize-cli db:migrate",
      "db:seed": "sequelize-cli db:seed:all",
      "db:reset": "sequelize-cli db:drop && sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all",
      "test": "jest"
    },
    "keywords": [
      "tacacs",
      "ldap",
      "authentication",
      "authorization",
      "monitoring",
      "audit"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
      "express": "^4.18.2",
      "sequelize": "^6.37.1",
      "mysql2": "^3.9.2",
      "jsonwebtoken": "^9.0.2",
      "bcrypt": "^5.1.1",
      "ldapjs": "^3.0.7",
      "nodemailer": "^6.9.10",
      "winston": "^3.11.0",
      "node-ssh": "^13.1.0",
      "dotenv": "^16.4.5",
      "joi": "^17.12.2",
      "cors": "^2.8.5",
      "helmet": "^7.1.0",
      "compression": "^1.7.4",
      "express-rate-limit": "^7.1.5",
      "multer": "^1.4.5-lts.1",
      "socket.io": "^4.7.4",
      "node-schedule": "^2.1.1",
      "exceljs": "^4.4.0",
      "pdfkit": "^0.14.0",
      "crypto-js": "^4.2.0"
    },
    "devDependencies": {
      "nodemon": "^3.1.0",
      "sequelize-cli": "^6.6.2",
      "jest": "^29.7.0",
      "supertest": "^6.3.4",
      "eslint": "^8.56.0"
    }
  }